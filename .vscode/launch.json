{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug Braavos Tests",
        "program": "${workspaceFolder}/node_modules/jest/bin/jest.js",
        "args": [
          "--runInBand",
          "test/unit-test/method/account/provider/braavos.spec.ts"
        ],
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "sourceMaps": true,
        "env": {
          "NODE_ENV": "test",
          "STARKNET_RPC_URL": "${env:STARKNET_RPC_URL}",
          "RUN_DEPLOYMENT_TEST": "true"
        }
      }
    ]
  }